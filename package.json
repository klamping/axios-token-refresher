{
  "name": "oauth-token-refresher",
  "version": "1.0.0",
  "description": "This library will add features for axios to refresh token.",
  "main": "index.js",
  "author": "Ramesh Ramalingam",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc index.ts --out ./dist/index.js",
    "execute": "node ./dist/index.js",
    "start": "yarn build && yarn execute",
    "watch-dist": "nodemon ./dist/index.js",
    "watch-src": "tsc index.ts -w  --out ./dist/index.js",
    "watch": "concurrently 'yarn watch-src' 'yarn watch-dist'"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1",
    "typescript": "^3.5.1"
  },
  "dependencies": {}
}
